% if(session 'uid') {
.visible-if-user { display: block }
.navbar .nav > li.visible-if-user { display: block }
.visible-no-user { display: none }
.navbar .nav > li.visible-no-user { display: none }
% } else {
.visible-if-user { display: none }
.navbar .nav > li.visible-if-user { display: none }
.visible-no-user { display: block }
.navbar .nav > li.visible-no-user { display: block }
% }
