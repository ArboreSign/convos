<li class="nick-init" data-network="<%= $network %>" data-target="<%= $target %>">
  %= timestamp $timestamp
  <span class="prefix">Participants are</span>
  <span>
    % my $i = 0;
    % for my $nick (@$nicks) {
      <a href="cmd:///query <%= $nick->{nick} %>"><%= join('', @$nick{qw( mode nick )}) %></a><%= ++$i == @$nicks ? '.' : ', ' %>
    % }
  </span>
</li>
