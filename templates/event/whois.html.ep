<li class="whois" data-target="any">
  <span class="prefix"><%= app->config->{name} %>:</span>
  <span>
    %= link_to $nick, 'channel.view', { cid => $cid, target => $nick }
    is <%=$user%>@<%= $host %> (<%= $realname %>).</span>
  </span>
  <span class="timestamp" title="<%= format_time $timestamp, '%d. %n' %>"><%= format_time $timestamp, '%d. %n %H:%M:%S' %></span>
</li>
