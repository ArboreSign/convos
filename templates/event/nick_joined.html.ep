<li class="muted" <%if(stash('cid') && stash('target')) {%>data-target="target_<%= $cid %>_<%= as_id $channel %>"<%}%>>
  <span class="prefix"><%= app->config->{name} %>:</span>
  <span><%= $nick %> joined <%= $channel %></span>
  <span class="timestamp"><%= format_time $timestamp, "%H:%M:%S"  %></span>
</li>
