<li class="nick-joined" data-cid="<%= $cid %>" data-target="<%= $target %>" data-nick="<%=$nick%>">
  <span class="prefix"><%= app->config->{name} %>:</span>
  <span><%= link_to $nick, { cid => $cid, target => $nick }, class => 'nick' %></b> joined <%= $target %></span>
  %= timestamp_span($timestamp)
</li>
