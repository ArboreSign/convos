<li class="muted topic" data-target="<%= as_id $cid, $target %>">
  <span class="prefix">Topic is</span>
% if(length $topic) {
  <span><%= $topic %></span>
% } else {
  <span>No topic.</span>
% }
  <span class="timestamp" title="<%= format_time $timestamp, '%d. %n' %>"><%= format_time $timestamp, '%H:%M:%S' %></span>
</li>
