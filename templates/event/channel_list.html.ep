<li class="channel-list" data-cid="<%= $cid %>" data-target="any">
  <div class="content">
    % my $i = 0;
    % for my $c (sort { $a->{name} cmp $b->{name} } @$channel_list) {
      <%= link_to $c->{name}, view => { cid => $cid, target => $c->{name} }, title => $c->{title} %><%= ++$i == @$nicks ? '.' : ', ' %>
    % }
  </div>
  %= timestamp_span($timestamp)
</li>
