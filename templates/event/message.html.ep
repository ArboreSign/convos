<li class="message<%= stash('highlight') ? ' highlight' : '' %>" data-target="<%= as_id $cid, $target %>">
  &lt;<span class="prefix"><%= $nick %></span>&gt;
  <span class="content"><%== $message %></span>
  <span class="timestamp"><%= format_time $timestamp, '%H:%M:%S' %></span>
  % if(stash('embed')) {
    <div class="embed"><%==stash('embed')%></div>
  % }
</li>
