<li class="message<%= $highlight ? ' highlight' : '' %>" data-target="<%= as_id $cid, $target %>">
  <span class="prefix">&lt;<%= $nick %>&gt;</span>
  <span class="content"><%== $message %></span>
  <span class="timestamp"><%= format_time $timestamp, '%H:%M:%S' %></span>
  % if(stash('embed')) {
    <div class="embed"><%==stash('embed')%></div>
  % }
</li>
